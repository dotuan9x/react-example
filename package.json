{
  "name": "react-example",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "./public/electron.js",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "7.14.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-classes": "7.14.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "15.12.2",
    "@types/react": "17.0.9",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "electron": "^13.2.1",
    "electron-builder": "^22.11.7",
    "electronmon": "^2.0.2",
    "eslint": "7.28.0",
    "eslint-plugin-react": "^7.20.6",
    "html-webpack-plugin": "^5.3.1",
    "ts-loader": "9.2.3",
    "typescript": "^4.3.2",
    "wait-on": "^6.0.0",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "scripts": {
    "start": "webpack serve --mode development --config scripts/webpack.config.js",
    "build": "webpack --mode production --config scripts/webpack.build.js",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:8080 && electronmon .\"",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "My App",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
